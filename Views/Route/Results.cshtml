@{
    ViewData["Title"] = "Search Results";
}

<div class="text-center">
    <h2>Search Results</h2>
    <ul>
        @foreach (var route in Model)
        {
            <li>
                <p>From: @route.StartLocation</p>
                <p>To: @route.EndLocation</p>
                <p>Distance: @route.Distance km</p>
            </li>
        }
    </ul>
</div>
